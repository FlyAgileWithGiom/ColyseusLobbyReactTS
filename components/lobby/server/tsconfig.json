{
  "compilerOptions": {
    "outDir": "dist",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext"],
    "target": "ESNext",
    "declaration": true,
    "removeComments": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": true,
    "allowJs": false,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "include": ["src/"]
}
